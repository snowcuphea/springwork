package app1;

public class MyBeanTest {

	public static void main(String[] args) {
		
		MyBeanStyleA obj = new MyBeanStyleA();
		run(obj);
		show(obj);
		

	}
	public static void run(MyBeanStyleA obj) {
		System.out.println("**************");
		obj.testHello("泅后");
		obj.testHello("泅后");
		System.out.println("**************");
	}
	public static void show(MyBeanStyleA obj) {
		System.out.println("**************");
		obj.testHello("泅后");
		obj.testHello("泅后");
		System.out.println("**************");
		
	}

}
