package app2;
/*
 * app1에 있는 코드를 수정해서,
 * oop가 적용되어 있는 프로그램
 *
 */


public class MyBeanTest {

	public static void main(String[] args) {
		
		MyBeanStyleA obj = new MyBeanStyleA();
		run(obj);
		show(obj);
		

	}
	public static void run(MyBeanStyleA obj) {
		System.out.println("**************");
		obj.testHello("현빈");
		obj.testHello("현빈");
		System.out.println("**************");
	}
	public static void show(MyBeanStyleA obj) {
		System.out.println("**************");
		obj.testHello("현빈");
		obj.testHello("현빈");
		System.out.println("**************");
		
	}

}
